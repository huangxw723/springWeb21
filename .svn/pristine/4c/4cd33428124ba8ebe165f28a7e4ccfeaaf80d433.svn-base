log4j.rootLogger=DEBUG,CONSOLE

log4j.logger.com.aoshi=DEBUG,CONSOLE,LFILE

log4j.appender.CONSOLE=org.apache.log4j.ConsoleAppender
log4j.appender.CONSOLE.layout.ConversionPattern=%d{yyyy HH:mm:ss} %-5p %l %n - %m%n
log4j.appender.CONSOLE.Target=System.out
log4j.appender.CONSOLE.layout=org.apache.log4j.PatternLayout

log4j.appender.LFILE=org.apache.log4j.DailyRollingFileAppender  
log4j.appender.LFILE.File=${catalina.home}/logs/redRainManage_
log4j.appender.LFILE.DatePattern=yyyy-MM-dd
log4j.appender.LFILE.layout=org.apache.log4j.PatternLayout  
log4j.appender.LFILE.layout.ConversionPattern=%d{yyyy HH:mm:ss} %-5p %l %n - %m%n


